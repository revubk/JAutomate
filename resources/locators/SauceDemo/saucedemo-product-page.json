{
    "page": "saucedemo-product-page",
    "url": "https://www.saucedemo.com/inventory.html",
    "loadableComponents": [],
    "elements": {
        "Header": {
            "Title": "//div[@class='app_logo']",
            "HamburgerIcon": "#react-burger-menu-btn",
            "CartIcon": ".shopping_cart_link"
        },
        "Hamburger": {
            "Logout": "#logout_sidebar_link",
            "AllItems": "#inventory_sidebar_link",
            "About": "#about_sidebar_link",
            "ResetAppStatus": "#reset_sidebar_link",
            "Close": "#react-burger-cross-btn"
        },
        "MainContainer" : {
            "Title":"[data-test='title']",
            "FilterButton":"[data-test='product-sort-container']",
            "CartIcon":"[data-test='shopping-cart-link']",
            "InventoryList":"[data-test='inventory-container']",
            "InventoryItem":"[data-test='inventory-item']",
            "ProductImage":"img[class='inventory_item_img']",
            "ProductName":"[data-test*='title-link']",
            "ProductDescription":"[data-test='inventory-item-name']",
            "ProductPrice":"[data-test='inventory-item-price']",
            "AddToCart":"[data-test*='add-to-cart-']"

        }
    }
}